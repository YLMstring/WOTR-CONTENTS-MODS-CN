[
 {
    "Key": "AlternateRacialTraits.Features.Human.AdoptiveParentage.Description",
    "ProcessTemplates": false,
    "zhCN": "选择一种不具备人类亚种的{g|Encyclopedia:Race}种族{/g}。你使用该种族的起始语言以及武器熟悉种族特性。如果该种族没有武器熟悉，你以奖励{g|Encyclopedia:Feat}专长{/g}的形式获得一种和该种族风格相符的“技能专攻”或者“武器专攻”。本特性替换人类的奖励专长"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.AdoptiveParentage.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "养子"
  },
  {
    "Key": "AlternateRacialTraits.Features.RacialWeaponFamiliarities.Halfling.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "半身人武器熟练"
  },
  {
    "Key": "AlternateRacialTraits.Features.RacialWeaponFamiliarities.Halfling.Description",
    "ProcessTemplates": false,
    "zhCN": "半身人擅长使用投石杖，并将任何名字中带有“半身人”的武器视作{g|Encyclopedia:Weapon_Proficiency}军用武器{/g}。"
  },
  {
    "Key": "AlternateRacialTraits.Features.RacialWeaponFamiliarities.Gnome.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "侏儒武器熟练"
  },
  {
    "Key": "AlternateRacialTraits.Features.RacialWeaponFamiliarities.Gnome.Description",
    "ProcessTemplates": false,
    "zhCN": "侏儒将任何名字中带有“侏儒”的武器视作{g|Encyclopedia:Weapon_Proficiency}军用武器{/g}。"
  }, 
  {
    "Key": "AlternateRacialTraits.Features.Human.DualTalent.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "双重天赋"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.DualTalent.Description",
    "ProcessTemplates": false,
    "zhCN": "某些人类熟练于最大程度发挥自己的天赋，他们选择两项{g|Encyclopedia:Ability_Scores}属性{/g}获得+2种族{g|Encyclopedia:Bonus}加值{/g}。该特性会取代单项属性+2，奖励专长以及奖励技能。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.DualTalent.DisplayName.DualTalentStrength",
    "ProcessTemplates": false,
    "zhCN": "双重天赋力量"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.DualTalent.DisplayName.DualTalentDexterity",
    "ProcessTemplates": false,
    "zhCN": "双重天赋敏捷"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.DualTalent.DisplayName.DualTalentConstitution",
    "ProcessTemplates": false,
    "zhCN": "双重天赋体质"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.DualTalent.DisplayName.DualTalentIntelligence",
    "ProcessTemplates": false,
    "zhCN": "双重天赋智力"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.DualTalent.DisplayName.DualTalentWisdom",
    "ProcessTemplates": false,
    "zhCN": "双重天赋感知"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.DualTalent.DisplayName.DualTalentCharisma",
    "ProcessTemplates": false,
    "zhCN": "双重天赋魅力"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.AwarenessFeature.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "洞察"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.AwarenessFeature.Description",
    "ProcessTemplates": false,
    "zhCN": "在所有的{g|Encyclopedia:Saving_Throw}豁免检定{/g}和{g|Encyclopedia:Concentration}专注检定{/g}上获得+1种族{g|Encyclopedia:Bonus}加值{/g}。本特性替换人类的奖励专长。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.ComprehensiveEducation.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "素质教育"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.ComprehensiveEducation.Description",
    "ProcessTemplates": false,
    "zhCN": "在熟练教师的帮助下成长的人类，会充分利用数百年文明中获得的大量知识。将所有知识技能视为{g|Encyclopedia:Skills}本职技能{/g}，并在所有他们通过职业等级获得作为本职技能的知识技能{g|Encyclopedia:Check}检定{/g}上获得+1种族加值。本特性替换奖励技能。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.FocusedStudyProgression.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "精研"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.FocusedStudyProgression.Description",
    "ProcessTemplates": false,
    "zhCN": "所有的人都多才多艺，但有些人不是通才，而是专精于某项{g|Encyclopedia:Skills}技能{/g}。在1级、8级以及16级时，选择一项技能，获得该技能的“技能专攻”专长。这个种族特性取代奖励专长。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.GiantAncestry.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "巨人祖先"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.GiantAncestry.Description",
    "ProcessTemplates": false,
    "zhCN": "拥有食人魔或巨魔血统的人类最终拥有笨重的身材和不协调的特征。在{g|Encyclopedia:Combat_Maneuvers}战技检定{/g}和{g|Encyclopedia:CMD}CMD{/g}上获得+1{g|Encyclopedia:Bonus}加值{/g}，但在隐匿检定上受到-2{g|Encyclopedia:Penalty}减值{/g}，这个种族特性取代奖励技能。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.HumanBonusFeat.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "奖励专长"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.HumanBonusFeat.Description",
    "ProcessTemplates": false,
    "zhCN": "人类在1级时选择一个额外{g|Encyclopedia:Feat}专长{/g}。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.HumanFeatureSelection.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "种族替换特性"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.HumanFeatureSelection.Description",
    "ProcessTemplates": false,
    "zhCN": "有以下种族特性可选"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.MilitaryTradition.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "战斗民族"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.MilitaryTradition.Description",
    "ProcessTemplates": false,
    "zhCN": "一些人类文化群体会培养所有儿童（或某一社会阶层的所有儿童）参军训练武力。根据他们的所属文化群体获得最多两件军用或异种武器的{g|Encyclopedia:.Weapon_Proficiency}擅长{/g}。本特性替换人类的奖励专长。"
  },
  {
    "Key": "AlternateRacialTraits.Features.WeaponProficiencySelections.MartialWeaponProficiencyDisplayName",
    "ProcessTemplates": false,
    "zhCN": "擅长军用武器"
  },
  {
    "Key": "AlternateRacialTraits.Features.WeaponProficiencySelections.MartialWeaponProficiencyDescription",
    "ProcessTemplates": false,
    "zhCN": "选择一种军用武器获得其擅长"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.PracticedHunter.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "老练猎手"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.PracticedHunter.Description",
    "ProcessTemplates": false,
    "zhCN": "在隐匿和学识（自然）{g|Encyclopedia:Check}检定{/g}上获得+2种族{g|Encyclopedia:Bonus}加值{/g}，并且隐匿和学识（自然）被他们视为本职技能。本特性替换奖励技能。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.UnstoppableMagic.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "法力澎湃"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.UnstoppableMagic.Description",
    "ProcessTemplates": false,
    "zhCN": "来自建立在高级魔法基础上的文明的人类会受到各种方式的教育，以实现他们的魔法追求。在穿透法术抗力的{g|Encyclopedia:Caster_Level}施法者等级{/g}{g|Encyclopedia:Check}检定{/g}上获得+2种族{g|Encyclopedia:Bonus}加值{/g}。本特性替换人类的奖励专长。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Oread.OreadFeatureSelection.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "种族替换特性"
  },
  {
    "Key": "AlternateRacialTraits.Features.Oread.OreadFeatureSelection.Description",
    "ProcessTemplates": false,
    "zhCN": "有以下种族特性可选"
  },
  {
    "Key": "AlternateRacialTraits.Features.Oread.NoAlternateTraits.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "无"
  },
  {
    "Key": "AlternateRacialTraits.Features.Oread.NoAlternateTraits.Description",
    "ProcessTemplates": false,
    "zhCN": "无替换特性"
  },
  {
    "Key": "AlternateRacialTraits.Features.Oread.CrystallineForm.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "水晶躯体"
  },
  {
    "Key": "AlternateRacialTraits.Features.Oread.CrystallineForm.Description",
    "ProcessTemplates": false,
    "zhCN": "由于反射性的水晶躯体，具有此特性的土巨灵裔在对抗射线时，AC上获得+2种族加值。此外，每日一次，他们可以反射对其释放的单发射线，如同使用了拨挡飞箭专长。该特性替代强酸亲和。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Oread.GraniteSkin.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "花岗岩肤"
  },
  {
    "Key": "AlternateRacialTraits.Features.Oread.GraniteSkin.Description",
    "ProcessTemplates": false,
    "zhCN": "具有此特性的土巨灵裔皮肤上覆盖着岩石，天生防御上获得+1种族加值。该特性替代能量抗性。"
  },
  {
    "Key": "AlternateRacialTraits.Features.HalfElf.EyeForOpportunity.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "灵机之眼"
  },
  {
    "Key": "AlternateRacialTraits.Features.HalfElf.EyeForOpportunity.Description",
    "ProcessTemplates": false,
    "zhCN": "不断在两个社会的边缘游走，一些半精灵擅长寻找那些被忽视的机会。他们在借机攻击上获得+1种族加值。该特性替代一技之长和敏锐直觉。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.EyeForTalent.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "天赋之眼"
  },
  {
    "Key": "AlternateRacialTraits.Features.Human.EyeForTalent.Description",
    "ProcessTemplates": false,
    "zhCN": "人类拥有强大、潜藏的直觉，他们在察言观色检定上获得+2种族加值，同时，如果他们有动物伙伴、奖励坐骑、追随者或是魔宠的话，这些生物可以选择一个属性值，让该属性得到+2的加值，这个种族特性取代奖励专长。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Aasimar.AasimarFeatureSelection.Description",
    "ProcessTemplates": false,
    "zhCN": "有以下种族特性可选"
  },
  {
    "Key": "AlternateRacialTraits.Features.Aasimar.AasimarFeatureSelection.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "种族替换特性"
  },
  {
    "Key": "AlternateRacialTraits.Features.Aasimar.CelestialCrusader.Description",
    "ProcessTemplates": false,
    "zhCN": "一些神裔选择遵从他们与生俱来对抗邪恶的命运。他们在面对邪恶异界生物时，在{g|Encyclopedia:Attack}攻击{/g}检定和{g|Encyclopedia:Armor_Class}AC{/g}上获得+1洞察{g|Encyclopedia:Bonus}加值{/g}；该种族{g|Encyclopedia:Trait}特性{/g}替换天界抗性和氏族技能奖励。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Aasimar.CelestialCrusader.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "天界先锋"
  },
  {
    "Key": "AlternateRacialTraits.Features.HeritageFeatures.SkilledPrerequisiteDisplayName",
    "ProcessTemplates": false,
    "zhCN": "受训"
  },
  {
    "Key": "AlternateRacialTraits.Features.HeritageFeatures.SLAPrerequisiteDisplayName",
    "ProcessTemplates": false,
    "zhCN": "类法术能力"
  },
  {
    "Key": "AlternateRacialTraits.Features.NoAdditionalTraitsPrototype.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "无特性"
  },
  {
    "Key": "AlternateRacialTraits.Features.NoAdditionalTraitsPrototype.Description",
    "ProcessTemplates": false,
    "zhCN": "不替换种族特性"
  },
  {
    "Key": "AlternateRacialTraits.Features.Aasimar.CrusadingMagic.Description",
    "ProcessTemplates": false,
    "zhCN": "许多神裔都觉得有义务受训以对抗从世界之伤入侵的魔族，守护这个世界。这些神裔在克服{g|Encyclopedia:Spell_Resistance}法术抗力{/g}的{g|Encyclopedia:Caster_Level}施法者等级{/g}和{g|Encyclopedia:Knowledge_Arcana}知识（神秘）{/g}检定获得+2种族{g|Encyclopedia:Bonus}加值{/g}。该种族{g|Encyclopedia:Trait}特性{/g}替换氏族技能奖励和类法术能力。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Aasimar.CrusadingMagic.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "圣战魔法"
  },
  {
    "Key": "AlternateRacialTraits.Features.Aasimar.DeathlessSpirit.Description",
    "ProcessTemplates": false,
    "zhCN": "一些意志特别坚强的神裔可以抵御死亡的力量。他们获得5点负能量{g|Encyclopedia:Energy_Resistance}抗力{/g}。当他们获得负向等级时将不会同时损失{g|Encyclopedia:HP}生命值{/g}，在针对即死效果、能量吸取、负能量和{g|Encyclopedia:Necromancy}死灵{/g}学派的{g|Encyclopedia:Spells}法术{/g}或类法术效果时，{g|Encyclopedia:Saving_Throw}豁免检定{/g}获得+2种族{g|Encyclopedia:Bonus}加值{/g}。该种族{g|Encyclopedia:Trait}特性{/g}替换天界抗性。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Aasimar.DeathlessSpirit.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "不灭之灵"
  },
  {
    "Key": "AlternateRacialTraits.Features.Aasimar.ExaltedResistance.Description",
    "ProcessTemplates": false,
    "zhCN": "在面对拥有邪恶{g|Encyclopedia:Spell_Descriptor}描述符{/g}的法术或类法术，或是由邪恶异界生物施放的{g|Encyclopedia:Spells}法术{/g}或类法术能力时，这项种族{g|Encyclopedia:Trait}特性{/g}将赋予神裔相当于5+角色{g|Encyclopedia:Character_Level}等级{/g}的{g|Encyclopedia:Spell_Resistance}法术抗力{/g}。该种族{g|Encyclopedia:Trait}特性{/g}替换天界抗性。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Aasimar.ExaltedResistance.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "崇善抗力"
  },
  {
    "Key": "AlternateRacialTraits.Features.Aasimar.Heavenborn.Description",
    "ProcessTemplates": false,
    "zhCN": "有些神裔在天界出生。该种族{g|Encyclopedia:Trait}特性{/g}让神裔在{g|Encyclopedia:Knowledge_Arcana}知识（神秘）{/g}检定上获得+2{g|Encyclopedia:Bonus}加值{/g}，并且他们在施放带有善良或光亮{g|Encyclopedia:Spell_Descriptor}描述符{/g}的{g|Encyclopedia:Spells}法术{/g}时视为{g|Encyclopedia:Caster_Level}施法者等级{/g}比实际高1级。该种族{g|Encyclopedia:Trait}特性{/g}取代氏族技能奖励和类法术能力。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Aasimar.Heavenborn.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "生于天界"
  },
  {
    "Key": "AlternateRacialTraits.Features.Aasimar.LostPromise.Description",
    "ProcessTemplates": false,
    "zhCN": "许多人把神裔的美态和天界之力当作一份礼物，但在一些社区中这份独特可能变成被迫害的原因，从而使神裔坠入黑暗。邪恶的力量对于他们的天界同行赐予的礼物被歪曲而感到愉悦。只要他们保持在邪恶{g|Encyclopedia:Alignment}阵营{/g}，他们获得魔裔的尖牙利爪种族替换特性。该种族{g|Encyclopedia:Trait}特性{/g}取代类法术能力。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Aasimar.LostPromise.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "失落之约"
  },
  {
    "Key": "AlternateRacialTraits.Features.Tiefling.TieflingFeatureSelection.Description",
    "ProcessTemplates": false,
    "zhCN": "有以下种族特性可选"
  },
  {
    "Key": "AlternateRacialTraits.Features.Tiefling.TieflingFeatureSelection.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "种族替换特性"
  },
  {
    "Key": "AlternateRacialTraits.Features.Tiefling.BeguilingLiar.Description",
    "ProcessTemplates": false,
    "zhCN": "许多魔裔发现世间最好的处事方法就是说别人想听的话。这些魔裔擅长欺骗敌人，并把谎言说得和真的一样，这使得他们在唬骗检定上获得+4种族{g|Encyclopedia:Bonus}加值{/g}。该种族{g|Encyclopedia:Trait}特性{/g}取代氏族技能奖励。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Tiefling.BeguilingLiar.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "妄语虚言"
  },
  {
    "Key": "AlternateRacialTraits.Features.Tiefling.FiendishSorcery.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "炼狱魔力"
  },
  {
    "Key": "AlternateRacialTraits.Features.Tiefling.MawOrClaw.Description",
    "ProcessTemplates": false,
    "zhCN": "有的魔裔呈现出更多他们残忍祖先的兽性特征。这些魔裔拥有强有力的尖牙或危险的利爪。魔裔可以选择造成1d6{g|Encyclopedia:Damage}伤害{/g}的啮咬或者两次每次造成1d4{g|Encyclopedia:Damage}伤害{/g}的爪抓{g|Encyclopedia:Attack}攻击{/g}。这些攻击方式属于{g|Encyclopedia:NaturalAttack}主要天生武器{/g}。该种族{g|Encyclopedia:Trait}特性{/g}取代类法术能力。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Tiefling.MawOrClaw.MawDisplayName",
    "ProcessTemplates": false,
    "zhCN": "尖牙利爪（啮咬）"
  },
  {
    "Key": "AlternateRacialTraits.Features.Tiefling.MawOrClaw.ClawDisplayName",
    "ProcessTemplates": false,
    "zhCN": "尖牙利爪（爪抓）"
  },
  {
    "Key": "AlternateRacialTraits.Features.Tiefling.MawOrClaw.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "尖牙利爪"
  },
  {
    "Key": "AlternateRacialTraits.Features.Tiefling.ScaledSkin.Description",
    "ProcessTemplates": false,
    "zhCN": "某些魔裔的皮肤给予他们能量抗力，同时也坚硬如甲。选择以下{g|Encyclopedia:Energy_Damage}能量{/g}类型之一：寒冷，电击或火焰。拥有该{g|Encyclopedia:Trait}特性{/g}的魔裔获得5点该种类型的能量{g|Encyclopedia:.Energy_Resistance}抗力{/g}，并且还在{g|Encyclopedia:.Armor_Class}AC{/g}上获得1点天生防御{g|Encyclopedia:Bonus}加值{/g}。该种族{g|Encyclopedia:Trait}特性{/g}取代魔界抗力。"
  },
  {
    "Key": "AlternateRacialTraits.Features.Tiefling.ScaledSkin.DisplayName",
    "ProcessTemplates": false,
    "zhCN": "鳞片皮肤"
  },
  {
    "Key": "AlternateRacialTraits.Features.Tiefling.ScaledSkin.DisplayName.Fire",
    "ProcessTemplates": false,
    "zhCN": "鳞片皮肤（火焰）"
  },
  {
    "Key": "AlternateRacialTraits.Features.Tiefling.ScaledSkin.DisplayName.Cold",
    "ProcessTemplates": false,
    "zhCN": "鳞片皮肤（寒冷）"
  },
  {
    "Key": "AlternateRacialTraits.Features.Tiefling.ScaledSkin.DisplayName.Electricity",
    "ProcessTemplates": false,
